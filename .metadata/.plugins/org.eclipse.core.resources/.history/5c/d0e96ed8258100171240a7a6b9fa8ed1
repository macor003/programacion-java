package com.macor.CodeWars;

public class SumOfNumbers {

	public static void main(String[] args) {
		int a = 1;
		int b = 2;

			
		
		int max = Math.max(a, b);
		int min = Math.min(a, b);
		int result = 0;
		
		for (min; min >= max; min++) {
			result += min;
		}

		System.out.println(result);

	}

}
