package com.macor.CodeWars;

public class SumOfNumbers {

	public static void main(String[] args) {
		int a = 0;
		int b = 1;

		int min = 0;
		int max = Math.max(a, b);
		int result = 0;
		
		for (min = Math.min(a, b); min < max; min++) {
			result += min;
		}

		System.out.println(result);

	}

}
